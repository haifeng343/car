.mask{
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  background-color: rgba(0, 0, 0, 0.3);
  z-index: 999;
}
.fixed-dialog{
  width: 80%;
  height: 740rpx;
  position: fixed;
  top: 50%;
  left: 50%;
  background: url(data:image/png;base64,) no-repeat bottom left content-box,url(data:image/png;base64,) no-repeat center top content-box;
  background-size: 65%,30%;
  transform: translate(-50%,-50%);
  background-color: #0948DE;
  border-radius: 8rpx;
  box-shadow: 2rpx 3rpx 8rpx rgba(0, 0, 0, 0.3);
}
.dialog-title{
  color: #fff;
  font-size: 48rpx;
  text-align: center;
  margin-top: 68rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.dialog-title .title1{
  display: block;
  margin: 0 16rpx;
}
.dialog-text{
  width: 484rpx;
  margin: 0 auto;
  font-size: 28rpx;
  color: #fff;
  text-indent: 2em;
  line-height: 36rpx;
  margin-top: 14rpx;
}
.dialog-content{
  width: 484rpx;
  height: auto;
  display: flex;
  margin: 30rpx auto 0;
  justify-content: space-between;
}
.dialog-guazi{
  width: 236rpx;
  border-radius: 2rpx;
  height: 236rpx;
  background-color: #fff;
  box-sizing: border-box;
  border: 4rpx solid #FFDA00;
  display: flex;
  align-items: center;
  text-align: center;
  margin: 0 auto;
}
.dialog-right{
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.dialog-youxin,.dialog-renren{
  width: 236rpx;
  height: 114rpx;
  box-sizing: border-box;
  background-color: #fff;
  border-radius: 2rpx;
  border: 4rpx solid #FFDA00;
  display: flex;
  align-items: center;
  text-align: center;
  margin: 0 auto;
}
.arrow{
  width: 28rpx;
  height: 30rpx;
}
.dialog-body{
  display: flex;
  margin: 0 auto;
  flex-direction: column;
  align-items: center;
}
.jiao{
  display: inline-block;
  width: 6rpx;
  height: 6rpx;
  background: #DA4E3A;
  position: relative;
  top: -4rpx;
  transform: rotate(45deg);
}
.jiao:nth-child(n+1){
  right: 16rpx;
}
.jiao:nth-child(n+2){
  left: 16rpx;
}
.dialog-carTitle{
  font-size: 36rpx;
  padding-right: 10rpx;
}
.dialog-number{
  font-size: 36rpx;
  color: #EB3F27;
}
.heng{
  height: 3rpx;
  position: relative;
  top: -20rpx;
}
.noNumber{
  opacity: 0.1;
}
.dialog-button{
  width: 482rpx;
  height: 92rpx;
  margin: 70rpx auto 0;
  background: linear-gradient(
    180deg,
    #FBD249 0%,
    #F4A423 100%
  );
  border-radius: 46rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 28rpx;
  color: #fff;
  font-weight: 400;
  position: relative;
}
.dialog-heng{
  font-weight: 100;
}