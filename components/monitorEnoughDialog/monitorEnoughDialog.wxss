.mask{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,0.5);
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 101
}
.top_image .image{width: 580rpx;height: 248rpx;vertical-align:top;}
.mask_out{
  width: 580rpx;
  background: #FFFFFF;
  border-radius: 0 0 16rpx 16rpx;
  position: relative;
  padding-bottom: 28rpx;
}
.mask .mask_n_type{padding-bottom: 30rpx}
.mask_title{font-size:40rpx;font-weight:500;color: #4A4A4A;text-align: center}
.mask_content{font-size:28rpx;color: #9B9B9B;text-indent: 52rpx;margin: 4rpx 34rpx 0 52rpx}
.mask_mold{margin: 20rpx 38rpx 0;}
.mask_mold .mold_first{display: flex;flex-wrap: wrap;position: relative;justify-content:space-between;}
.mask_mold .mold_one{justify-content:center}
.mold{width:238rpx ;height: 188rpx;position: relative;}
.mold .mold_name{width:238rpx ;height: 188rpx;}
.mold .image{width:238rpx;height: 188rpx;}
.mold .mold_name .num{font-size:36rpx;color: #FFFFFF;font-weight:500; position: absolute;  bottom: 40rpx;width:238rpx;text-align: center}
.mask_mold .mold_three .mold_normal{display: flex;height: 238rpx;justify-content: center;align-items: center}
.mask_mold .mold_three .mold_other{display: flex;height: 108rpx;justify-content: center;align-items: center}
.mask_mold .mold_three .mold_normal .image{height: 238rpx;border-radius: 8rpx;}
.mask_mold .mold_three .mold_other .image{height: 108rpx;border-radius: 8rpx;}
.mask_mold .mold_three .mold_other .num{font-size:32rpx;}
.mask_mold .mold_three .mold_ab{position: absolute;right: 0;bottom: 0;}

.mask_mold .mold_three .mold_normal .num{bottom: 56rpx;}
.mask_mold .mold_three .mold_normal .mold_name{height: 238rpx;}
.mask_mold .mold_three .mold_normal .line{width: 140rpx;height: 2rpx;position: absolute; background: #FFFFFF;left: 50%;top:50%;margin-left: -70rpx}
.mask_mold .mold_three .mold_normal .gz_logo{width: 68rpx;height: 28rpx;position: absolute;  top: 70rpx;left: 66rpx;}
.mask_mold .mold_three .mold_normal .yx_logo{width: 88rpx;height: 28rpx;position: absolute;  top: 70rpx;left: 54rpx;}
.mask_mold .mold_three .mold_normal .rr_logo{width: 92rpx;height: 28rpx;position: absolute;  top: 70rpx;left: 54rpx;}
.mask_mold .mold_three .mold_normal .arrow{width: 28rpx;height: 28rpx;position: absolute;}
.mask_mold .mold_three .mold_normal .gz_arrow{top: 70rpx;right: 54rpx;}
.mask_mold .mold_three .mold_normal .yx_arrow{top: 70rpx;right: 40rpx;}
.mask_mold .mold_three .mold_normal .rr_arrow{top: 70rpx;right: 40rpx;}

.mask_mold .mold_three .mold_other .num{bottom: 8rpx;}
.mask_mold .mold_three .mold_other .mold_name{height: 108rpx;}
.mask_mold .mold_three .mold_other .line{width: 140rpx;height: 2rpx;position: absolute; background: #FFFFFF;left: 50%;top:50%;margin-left: -70rpx}
.mask_mold .mold_three .mold_other .gz_logo{width: 68rpx;height: 28rpx;position: absolute;  top: 16rpx;left: 66rpx;}
.mask_mold .mold_three .mold_other .yx_logo{width: 88rpx;height: 28rpx;position: absolute;  top: 16rpx;left: 54rpx;}
.mask_mold .mold_three .mold_other .rr_logo{width: 92rpx;height: 28rpx;position: absolute;  top: 16rpx;left: 54rpx;}
.mask_mold .mold_three .mold_other .arrow{width: 28rpx;height: 28rpx;position: absolute;}
.mask_mold .mold_three .mold_other .gz_arrow{top: 16rpx;right: 54rpx;}
.mask_mold .mold_three .mold_other .yx_arrow{top: 16rpx;right: 40rpx;}
.mask_mold .mold_three .mold_other .rr_arrow{top: 16rpx;right: 40rpx;}
.mask_mold .mold_three .mold_other .gz_un_logo{opacity: 0.4;left: 50%;margin-left: -34rpx;}
.mask_mold .mold_three .mold_other .yx_un_logo{opacity: 0.4;left: 50%;margin-left: -44rpx;}
.mask_mold .mold_three .mold_other .rr_un_logo{opacity: 0.4;left: 50%;margin-left: -46rpx;}
.mask_out .mask_know{
  width: 482rpx;
  height: 92rpx;
  background:rgba(0,80,254,1);
  box-shadow:2px 2px 4px 0px rgba(0,116,188,1);
  color: #FFFFFF;
  margin: 42rpx auto 0;
  border-radius: 46rpx;
  text-align: center;
  line-height: 92rpx;
  position: relative;
}
