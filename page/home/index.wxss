@import "../../assets/styles/font.wxss";

.section {
    height: 100%;
    background: #f9f9f9;
    display: flex;
    justify-content: center;
    align-items: center;
    flex-direction: column;
}

.banner {
    width: 750rpx;
    height: 468rpx;
    position: relative;
    z-index: 1;
}

.banner-image {
    width: 750rpx;
    height: 468rpx;
}

.banner .wx-swiper-dots.wx-swiper-dots-horizontal{
    margin-bottom: 200rpx;
    /* margin-left: 250rpx; */
}
.banner .wx-swiper-dot{
    width:20rpx;
    display: inline-flex;
    height: 4rpx;
    margin-left: 2rpx;
    justify-content:space-between;
    background: rgba(255,255,255,0.65);
    border-radius: 2rpx;
}
.banner .wx-swiper-dot-active{
    content: '';
    flex-grow: 1;
    background:rgba(255,255,255,1);
}

.content {
    margin: -172rpx auto 20rpx;
    width: 710rpx;
    flex: 1;
    z-index: 2;
    border-radius: 12rpx;
    box-shadow: 6rpx 10rpx 20rpx 0rpx rgba(0, 0, 0, 0.07);
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.search-box {
    width: 100%;
    flex: 1;
    margin: 0 auto;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    align-items: center;
    position: relative;
    padding-bottom: 140rpx;
    border-radius: 12rpx;
    overflow: hidden;
}

.search-content {
    width: 100%;
    flex: 1;
    background-color: #fff;
    padding: 0;
    box-sizing: border-box;
    position: relative;
    z-index: 33;
}
.search-content-main {
    width: 100%;
    height: 100%;
    overflow-y: auto;
    padding: 0;
    box-sizing: border-box;
    opacity: 1;
    position: absolute;
    top: 0;
    left: 0;
    transition: opacity 0.7s ease-in;
    z-index: 1;
}
.search-content-main.hidden {
    opacity: 0;
    z-index: -1;
    transition: opacity 0.1s ease-in;
}
.search_item {
    height: 100rpx;
    border-bottom: 2rpx solid #f4f4f4;
    display: flex;
    justify-content: space-between;
    align-items: center;
    flex-wrap: wrap;
    position: relative;
    margin: 0 32rpx;
    box-sizing: border-box;
}
.search_item.city {
    height: 120rpx;
}
.search_item_spread {
    color: #0050FE;
    text-align: center;
    margin: 30rpx 0;
}
.search_item_spread .icon-font {
    margin-top: 10rpx;
    font-size: 24rpx;
}
.search_item .tag {
    position: absolute;
    top: 20rpx;
    right: 0;
    font-size: 30rpx;
    font-weight: 400;
    color:rgba(0,80,254,1);
}
.search_item .sort .uniE930 {
    display: inline-block;
    width: 24rpx;
    height: 24rpx;
    color: #fff;
    line-height: 28rpx;
    font-size: 18rpx;
    text-align: center;
    background-color: #fff;
    border: 2rpx solid #e75d28;
    border-radius: 8rpx;
}
.search_item .sort .uniE930.active {
    color: #fff;
    background-color: #e75d28;
}
.search_item.noBorder {
    border: none;
    height: auto;
    margin-bottom: 10rpx;
    padding-top: 20rpx;
}
.search_item .position {
    flex: 1;
    font-size: 32rpx;
    color: #1d1d1d;
    line-height: 108rpx;
    font-weight: bold;
}
.search_item .positionByHand {
    flex: 0 0 auto;
    width: 250rpx;
    line-height: 108rpx;
    font-size: 22rpx;
    color: #1d1d1d;
    text-align: right;
    box-sizing: border-box;
    margin-right: 12rpx;
    height: 100%;
    display: flex;
    justify-content: flex-end;
    align-items: center;
}
.search_item .positionByHand .icon_img {
    width: 28rpx;
    height: 38rpx;
    margin-left: 10rpx;
}

.search_item .scenic {
    flex: 1;
    font-size: 32rpx;
    padding-right: 60rpx;
    display: flex;
    align-items: center;
    position: relative;
    height: 100%;
    width: 100%;
    box-sizing: border-box;
}

.scenic-text {
    width: 100%;
    display: flex;
    justify-content: flex-start;
    align-items: center;
    color: #0050FE;
    font-size: 32rpx;
}
.scenic-text.hidden {
    color: #bec3cd;
}
.scenic-text .logo_img {
    width: 80rpx;
    height: 80rpx;
    border-radius: 50%;
    margin-right: 10rpx;
    overflow: hidden;
}
.scenic-text .text {
    flex: 1;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.search_item .scenic .icon-font {
    font-size: 28rpx;
    position: absolute;
    right: 0;
    display: flex;
    align-items: center;
    justify-content: center;
    padding: 0 24rpx;
    color: #bec3cd;
    z-index: 333;
    top: 0;
    height: 100%;
}

.search_item .title {
    width: 100%;
    font-size: 32rpx;
    font-weight: bold;
    margin-bottom: 22rpx;
    display: flex;
    justify-content: flex-start;
    align-items: center;
}
.search_item .title .title_icon_img {
    width: 22rpx;
    height: 28rpx;
    margin-right: 8rpx;
}
.search_item .unit {
    color: #9B9B9B;
    margin-left: 4rpx;
}
.search_item .item_view_back {
    width: 100%;
    display: flex;
    display: -webkit-flex;
    /*justify-content: space-between;*/
    justify-content: flex-start;
    align-items: center;
    flex-direction: row;
    flex-wrap: wrap;
}
.search_item .item_view {
    flex: 0 0 auto;
    height: 40rpx;
    width: 144rpx;
    border-radius: 40rpx;
    background: rgba(240,240,240,1);
    text-align: center;
    line-height: 40rpx;
    margin-right: 20rpx;
    margin-bottom: 20rpx;
}
.search_item .item_view:nth-child(4n+4) {
    margin-right: 0;
}
.search_item .item_view.active {
    background-color: #0050FE;
    color: #fff;
}
.search_item .leaseType {
    flex: 0 0 auto;
    width: 156rpx;
    height: 40rpx;
    border-radius: 40rpx;
    background: #f0f0f0;
    text-align: center;
    line-height: 40rpx;
    margin-right: 16rpx;
}
.search_item .leaseType.active {
    background-color: #f1612a;
    color: #fff;
}
.search_item .equipments {
    font-size: 28rpx;
    color: #1d1d1d;
    width: 25%;
    text-align: center;
    margin-bottom: 18rpx;
}
.search_item .equipments .equipments-img {
    background: #fff6f2;
    width: 96rpx;
    height: 96rpx;
    border-radius: 50%;
    margin: 0 auto 6rpx;
    color: #1d1d1d;
    font-size: 46rpx;
    text-align: center;
    line-height: 96rpx;
}
.search_item .equipments.active .equipments-img {
    background: #f1612a;
    color: white;
}
.search-btn-back {
    position: absolute;
    width: 710rpx;
    height: 146rpx;
    left: 0;
    bottom: 0;
    padding-top: 30rpx;
    box-sizing: border-box;
    background: white;
    border-bottom-left-radius: 12rpx;
    border-bottom-right-radius: 12rpx;
}
.search_item .flex_item {
    width: 100%;
    padding-top: 20rpx;
    display: flex;
    justify-content: space-around;
    align-items: center;
    font-size: 28rpx;
    font-weight: 400;
    color: rgba(29, 29, 29, 1);
    line-height: 40rpx;
}
.search_item .flex_item .item_input {
    width: 280rpx;
    font-size: 32rpx;
    font-weight: 400;
    color: rgba(29, 29, 29, 1);
    line-height: 44rpx;
    border: none;
    border-bottom: solid 2rpx #e9e9e9;
}
.search_item .flex_item .item_input_placeholder {
    color: rgba(190, 195, 205, 1);
}
.search-btn {
    width: 480rpx;
    height: 92rpx;
    line-height: 92rpx;
    background: #0050FE;
    box-shadow: 0 4rpx 6rpx 0 rgba(0,105,170,1);
    border-radius: 92px;
    font-size: 28rpx;
    color: #fff;
    text-align: center;
    margin: 0 auto;
}
.select-button {
    width: 312rpx;
    height: 96rpx;
    background: rgba(255, 255, 255, 1);
    border-radius: 6rpx;
    border: 2rpx solid rgba(243, 243, 243, 1);
    padding: 10rpx 0 10rpx 42rpx;
    box-sizing: border-box;
    margin: 34rpx 0 6rpx;
}
.select-button.active {
    background: rgba(255, 214, 198, 1);
    border: 2rpx solid rgba(241, 97, 42, 1);
}
.select-button .select-button-title {
    font-size: 28rpx;
    font-weight: 600;
    color: rgba(29, 29, 29, 1);
    line-height: 40rpx;
}
.select-button .select-button-info {
    font-size: 24rpx;
    font-weight: 400;
    color: rgba(190, 195, 205, 1);
    line-height: 34rpx;
}
.select-button.active .select-button-info {
    color: rgba(241, 97, 42, 1);
}
::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
    display: none;
}

.range_back {
    height:154rpx;
    width:100%;
}

.tipDialog {
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 9999;
  display: flex;
  align-items: center;
  justify-content: center;
}

.dialog-overlay {
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  background-color: rgba(0, 0, 0, 0.4);
  z-index: 11;
}

.dialog-container {
  width: 75%;
  background-color: #fff;
  border-radius: 16rpx;
  position: relative;
  z-index: 22;
  padding: 0 60rpx;
  box-sizing: border-box;
}

.dialog-container .dialog-title {
  height: 120rpx;
  border-bottom: solid 2rpx #fde1d7;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 36rpx;
  font-weight: 500;
  color: #1d1d1d;
}

.dialog-container .dialog-content {
  font-size: 24rpx;
  height: auto;
  box-sizing: border-box;
  padding: 40rpx 0 30rpx;
}
.dialog-container .dialog-content .title {
  display: block;
  line-height: 180%;
}

.dialog-container .dialog-button {
  width: 100%;
  display: flex;
  align-items: flex-start;
  justify-content: center;
  height: 132rpx;
}

.dialog-container .dialog-button .button {
  width: 100%;
  height: 92rpx;
  background: linear-gradient(180deg, #0050fe 0%, #0050fe 100%);
  box-shadow: 4rpx 4rpx 8rpx 0rpx #cbeafd;
  border-radius: 46rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 28rpx;
  color: #fff;
  font-weight: 400;
  position: relative;
}

.dialog-container .dialog-button .button:active {
  top: 2rpx;
}