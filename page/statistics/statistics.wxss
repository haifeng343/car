.section {
  padding-bottom: 128rpx;
}

.section_bottom {
  padding-bottom: 0;
}

.statistics_query, .house_low_price, .statistics_detail {
  font-size: 32rpx;
  color: #1d1d1d;
  padding: 24rpx 0 0;
  background: #fff;
}

.house_low {
  margin: 0 28rpx;
  display: flex;
  align-items: center;
}

.house_low .price {
  width: 22rpx;
  height: 28rpx;
  margin-right: 8rpx;
}

.line {
  width: 100%;
  height: 12rpx;
  background: #f9f9f9;
  overflow-x: hidden;
}

.monit_start {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  height: 128rpx;
  background: #fff;
  border-top: 1px solid #e8e8e8;
  line-height: 128rpx;
  z-index: 22;
}

.monit_start view {
  margin: 0 28rpx;
}

.monit_start .monit_btn {
  width: 240rpx;
  height: 92rpx;
  background: #0050fe;
  box-shadow: 4rpx 4rpx 8rpx 0px #0074bc;
  display: inline-block;
  color: #fff;
  border-radius: 46rpx;
  line-height: 64rpx;
  text-align: center;
  position: absolute;
  right: 28rpx;
  top: 50%;
  margin-top: -46rpx;
  font-size: 28rpx;
}

.monit_start .price {
  color: #ff6363;
}

.monit_start text:nth-child(2) {
  margin-right: 20rpx;
}

.monit_stop text:nth-child(2) {
  margin-right: 48rpx;
}

.monit_stop text:nth-child(3) {
  margin-right: 0;
}

.monit_save text:nth-child(2) {
  margin-right: 0;
}

.monit_save text:nth-child(3) {
  margin-right: 0;
}

.monit_save .monit_btn {
  width: 324rpx;
  right: 38rpx;
}

.monit_start text.time_line, .monit_stop text.time_line {
  margin-right: 8rpx;
}

.monit_save .monit_back {
  width: 324rpx;
  background: #fff;
  color: #e75d28;
  border: 1px solid #e75d28;
  left: 38rpx;
  box-shadow: none;
}

.monit_save .monit_update {
  box-shadow: none;
}

.house_query {
  margin: 0 28rpx 20rpx;
  display: flex;
  align-items: center;
  color: #1d1d1d;
}

.house_query .query {
  width: 28rpx;
  height: 28rpx;
  margin-right: 8rpx;
}

.house_data {
  margin: 0 28rpx;
  border-bottom: 1rpx solid #e8e8e8;
  padding-bottom: 20rpx;
}

.house_data .data_first, .house_data .data_sencond {
  display: flex;
  color: #1d1d1d;
}

.house_data .data_first {
  margin-bottom: 10rpx;
}

.data_first .data_layout, .data_sencond .data_layout {
  flex: 1;
  height: 128rpx;
  padding: 18rpx 0 0 34rpx;
  background-repeat: no-repeat;
  background-size: 100% 100%;
  background-image: url('data:image/png;base64,');
}

.data_first .data_layout:first-child, .data_sencond .data_layout:first-child {
  margin-right: 10rpx;
}

.data_layout .real_num {
  font-size: 48rpx;
  color: #f8685f;
}

.data_layout .average_price {
  font-size: 48rpx;
  color: #f0b258;
}

.data_layout .total_num {
  font-size: 48rpx;
  color: #63c559;
}

.data_layout .low_price {
  font-size: 48rpx;
  color: #79aaff;
}

.data_first .data_average {
  background-image: url('data:image/png;base64,');
}

.data_sencond .data_total {
  background-image: url('data:image/png;base64,');
}

.data_sencond .data_low {
  background-image: url('data:image/png;base64,');
}

.house_stat {
  margin: 0 28rpx;
  display: flex;
  align-items: center;
  border-bottom: 1rpx solid #e8e8e8;
  padding-bottom: 20rpx;
  color: #1d1d1d;
}

.stat_card {
  margin: 24rpx 28rpx 40rpx;
}

.stat_card .stat_first {
  display: flex;
  color: #1d1d1d;
  flex-wrap: wrap;
  position: relative;
  justify-content: space-between;
}

.stat_first .stat_layout {
  width: 334rpx;
  height: 252rpx;
  margin-bottom: 26rpx;
  position: relative;
}

.stat_first .stat_layout image {
  width: 100%;
  height: 252rpx;
  border-radius: 8rpx;
}

.stat_first .stat_layout .no_text {
  position: absolute;
  bottom: 46rpx;
  left: 50%;
  font-size: 24rpx;
  font-weight: 500;
  color: #fff;
  text-shadow: 0px 1px 1px rgba(0, 0, 0, 0.5);
  width: 96rpx;
  margin-left: -48rpx;
}

.stat_three .stat_layout .no_text {
  bottom: 20rpx;
}

.stat_three .stat_normal {
  height: 382rpx;
}

.stat_three .stat_normal image {
  height: 382rpx;
}

.stat_three .stat_other {
  height: 180rpx;
}

.stat_three .stat_other image {
  height: 180rpx;
}

.stat_three .stat_ab {
  position: absolute;
  right: 0;
  bottom: 0;
}

.stat_two .stat_layout {
  height: 382rpx;
}

.stat_two .stat_layout image {
  height: 382rpx;
}

.stat_one .stat_layout {
  width: 100%;
  height: 382rpx;
}

.stat_one .stat_layout image {
  height: 382rpx;
}

.stat_layout .stat_tip {
  position: absolute;
  background: rgba(255, 255, 255, 0.8);
  width: 280rpx;
  height: 88rpx;
  bottom: 18rpx;
  left: 50%;
  margin-left: -140rpx;
  border-radius: 6rpx;
  font-size: 26rpx;
  color: #1d1d1d;
  text-align: center;
}

.stat_layout .tip {
  width: 112rpx;
  height: 44rpx;
  position: absolute;
  top: 0;
  left: 50%;
  margin-left: -56rpx;
  text-align: center;
  line-height: 42rpx;
  font-size: 26rpx;
  color: #fff;
  background-repeat: no-repeat;
  background-size: 100% 100%;
}

.stat_layout .tip1 {
  background-image: url('data:image/png;base64,') !important;
}

.stat_layout .tip2 {
  background-image: url('data:image/png;base64,') !important;
}

.stat_layout .tip3 {
  background-image: url('data:image/png;base64,') !important;
}

.stat_layout .stat_tip .price {
  margin-top: 8rpx;
}

.stat_layout .stat_tip .num {
  font-size: 22rpx;
  color: #9f9f9f;
}

.gz {
  position: relative;
  display: flex;
  align-items: center;
  margin: 0 auto;
  justify-content: center;
}

.gz_logo {
  position: absolute;
  width: 102rpx !important;
  height: 48rpx !important;
  opacity: 0.4 !important;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
}

.rr_logo {
  position: absolute;
  width: 138rpx !important;
  opacity: 0.4 !important;
  height: 42rpx !important;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
}

.yx_logo {
  position: absolute;
  width: 132rpx !important;
  height: 42rpx !important;
  opacity: 0.4 !important;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
}
